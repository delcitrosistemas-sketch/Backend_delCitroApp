{
  "version": 2,
  "scripts": {
    "build": "nest build && npm run vercel:build",
    "vercel:build": "cp vercel.json dist/ && cp -r prisma dist/",
    "start": "node dist/main",
    "dev": "nest start --watch"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
    }
  ]
}
